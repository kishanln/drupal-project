{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [{
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/bartik": "^1.0",
        "drupal/better_exposed_filters": "^6.0.2",
        "drupal/blazy": "^2.11",
        "drupal/block_exclude_pages": "^2.0",
        "drupal/ckeditor": "^1.0",
        "drupal/classy": "^1.0",
        "drupal/color": "^1.0",
        "drupal/colorbox": "^2.0",
        "drupal/commerce": "2.38",
        "drupal/commerce_cart_redirection": "^3.0",
        "drupal/commerce_product_limits": "^1.0",
        "drupal/commerce_recurring": "^1.0@beta",
        "drupal/commerce_stripe": "^1.1",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.2",
        "drupal/core-composer-scaffold": "^10.0.0",
        "drupal/core-project-message": "^10.0.0",
        "drupal/core-recommended": "^10.0",
        "drupal/crop": "^2.3",
        "drupal/ctools": "^3.7",
        "drupal/custom_search": "^1.0@beta",
        "drupal/date_range_formatter": "^4.0",
        "drupal/devel": "^5.1",
        "drupal/entity_limit": "^3.0@beta",
        "drupal/fb_likebox": "^2.2",
        "drupal/field_group": "^3.2",
        "drupal/field_permissions": "^1.1",
        "drupal/focal_point": "^2.0",
        "drupal/geocoder": "4.23",
        "drupal/google_analytics": "^4.0",
        "drupal/htmlmail": "4.0.x-dev@dev",
        "drupal/image_widget_crop": "^2.3",
        "drupal/ip2country": "^2.0",
        "drupal/jcarousel": "6.0.0-alpha1",
        "drupal/jquery_ui_tabs": "^2.0",
        "drupal/jquery_ui_tooltip": "^2.0",
        "drupal/leaflet": "10.2",
        "drupal/libraries": "^4.0",
        "drupal/mailchimp": "^2.0",
        "drupal/masquerade": "^2.0@RC",
        "drupal/maxlength": "^2.1",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_per_role": "^1.3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/nocurrent_pass": "^1.1",
        "drupal/owlcarousel": "^3.0",
        "drupal/owlcarousel2": "^2.0",
        "drupal/pathauto": "^1.10",
        "drupal/paypal_subscriptions": "^2.0",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.0",
        "drupal/redirect_after_login": "^2.7",
        "drupal/reroute_email": "^2.0",
        "drupal/responsive_menu": "^4.4",
        "drupal/restrict_ip": "4.0.0-beta1",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/seven": "^1.0",
        "drupal/simple_gmap": "3.1",
        "drupal/slick": "^2.6",
        "drupal/slick_views": "^2.6",
        "drupal/smart_ip": "^4.2",
        "drupal/smtp": "^1.0",
        "drupal/stable": "^2.0",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/superfish": "^1.4",
        "drupal/twitter_block": "^3.0@alpha",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.0",
        "drupal/user_display_name": "1.1",
        "drupal/video_embed_field": "^2.5",
        "drupal/views_conditional": "^1.4",
        "drupal/views_display_switch": "^1.1",
        "drupal/views_slideshow": "^5.0",
        "drupal/webform": "^6.1",
        "drush/drush": "^12.4",
        "geocoder-php/google-maps-provider": "^4.7",
        "quickpay/quickpay-php-client": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
     "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "palantirnet/drupal-rector": "^0.19.2"
    }
}
